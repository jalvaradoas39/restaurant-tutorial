{
	"name": "restaurant-tutorial",
	"version": "1.0.0",
	"description": "Tutorial for restaurant site using MERN stack",
	"main": "server.js",
	"engines": {
		"node": "12.14.0",
		"npm": "6.13.4"
	},
	"scripts": {
		"start": "node server.js",
		"start-server": "nodemon server.js --ignore './client/'",
		"start-client": "npm start --prefix client",
		"dev": "concurrently \"npm run start-server\" \"npm run start-client\""
	},
	"author": "Jorge Alvarado",
	"license": "MIT",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"express-validator": "^6.14.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.13.14",
		"multer": "^1.4.4",
		"nodemon": "^2.0.16",
		"react-redux": "^7.2.8",
		"redux": "^4.2.0",
		"redux-devtools-extension": "^2.13.9",
		"redux-thunk": "^2.4.1"
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"minimist": "^1.2.6",
		"morgan": "^1.10.0"
	}
}
